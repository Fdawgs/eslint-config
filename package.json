{
	"name": "@fdawgs/eslint-config",
	"version": "2.0.0",
	"description": "@fdawgs' shareable flat ESLint config",
	"keywords": [
		"eslint",
		"eslint-config"
	],
	"main": "src/index.mjs",
	"type": "module",
	"types": "types/index.d.mts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Fdawgs/eslint-config.git"
	},
	"homepage": "https://github.com/Fdawgs/eslint-config",
	"bugs": {
		"url": "https://github.com/Fdawgs/eslint-config/issues"
	},
	"license": "MIT",
	"author": "Frazer Smith <frazer.dev@icloud.com>",
	"funding": "https://github.com/sponsors/Fdawgs",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"lint:eslint:fix": "npm run lint:eslint -- --fix",
		"lint:eslint": "eslint . --cache",
		"lint:fix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
		"lint:licenses": "licensee --errors-only --production",
		"lint:prettier:fix": "prettier . -w -u",
		"lint:prettier": "prettier . -c -u",
		"lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:licenses",
		"test": "npm run lint",
		"typecheck": "tsc"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"devDependencies": {
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@eslint/compat": "^1.4.1",
		"@types/eslint-plugin-security": "^3.0.0",
		"@types/node": "^24.9.2",
		"licensee": "^11.1.1",
		"prettier": "^3.6.2",
		"typescript": "~5.9.3"
	},
	"peerDependencies": {
		"eslint": "^9.0.0"
	},
	"dependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
		"@eslint/js": "^9.38.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-jsdoc": "^61.1.10",
		"eslint-plugin-n": "^17.23.1",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-regexp": "^2.10.0",
		"eslint-plugin-security": "^3.0.1",
		"globals": "^16.4.0"
	}
}
